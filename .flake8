[flake8]
max-line-length = 127
extend-ignore = E203, E266, E501, W503
exclude = 
    .git,
    __pycache__,
    build,
    dist,
    .eggs,
    *.egg-info,
    .venv,
    venv,
    .pytest_cache,
    .mypy_cache,
    .tox
max-complexity = 10
per-file-ignores =
    __init__.py:F401
    tests/*:S101


